# Configuration sécurisée pour Herald.lol

# URL de l'API backend
VITE_API_URL=https://herald.lol

# Clés de sécurité (À CHANGER EN PRODUCTION)
REACT_APP_JWT_SECRET=your-super-secure-jwt-secret-key-change-in-production-256-bits
REACT_APP_CSRF_SECRET=your-super-secure-csrf-secret-key-change-in-production

# Configuration de sécurité
REACT_APP_SESSION_TIMEOUT=30
REACT_APP_SECURITY_LEVEL=high

# Configuration des logs de sécurité
REACT_APP_ENABLE_SECURITY_LOGS=true
REACT_APP_LOG_LEVEL=info

# Configuration HTTPS
HTTPS=true
SSL_CRT_FILE=./ssl/cert.pem
SSL_KEY_FILE=./ssl/key.pem