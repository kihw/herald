# Configuration OAuth Google
# Obtenez ces valeurs depuis https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URL=https://herald.lol/auth/google/callback

# Configuration API Riot Games
RIOT_API_KEY=your_riot_api_key_here

# Configuration serveur
PORT=8000
GIN_MODE=release

# Configuration base de données (optionnel)
DB_PATH=./data/herald.db

# Configuration SSL (optionnel pour développement local)
SSL_CERT_PATH=/etc/ssl/herald.crt
SSL_KEY_PATH=/etc/ssl/herald.key