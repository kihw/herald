          "pm.globals.set('timestamp', new Date().toISOString());",
          "",
          "// Auto-set API key from environment if available",
          "if (pm.environment.get('HERALD_API_KEY')) {",
          "    pm.collectionVariables.set('apiKey', pm.environment.get('HERALD_API_KEY'));",
          "}",
          "",
          "// Auto-set bearer token from environment if available",
          "if (pm.environment.get('HERALD_BEARER_TOKEN')) {",
          "    pm.collectionVariables.set('bearerToken', pm.environment.get('HERALD_BEARER_TOKEN'));",
          "}"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global response time test",
          "pm.test('Response time is acceptable', () => {",
          "    pm.expect(pm.response.responseTime).to.be.below(5000); // 5 seconds max",
          "});",
          "",
          "// Global rate limit header test",
          "if (pm.response.headers.get('X-Gaming-Rate-Limit')) {",
          "    pm.test('Rate limit headers are present', () => {",
          "        pm.expect(pm.response.headers.get('X-Gaming-Rate-Limit')).to.exist;",
          "        pm.expect(pm.response.headers.get('X-Gaming-Rate-Remaining')).to.exist;",
          "    });",
          "}",
          "",
          "// Global error handling",
          "if (pm.response.code >= 400) {",
          "    pm.test('Error response has gaming platform identifier', () => {",
          "        const response = pm.response.json();",
          "        pm.expect(response.gaming_platform).to.eql('herald-lol');",
          "    });",
          "}"
        ]
      }
    }
